                <owl:onProperty rdf:resource="http://www.snik.eu/ontology/meta/isAssociatedWith"/>
                <owl:someValuesFrom rdf:resource="http://www.snik.eu/ontology/it4it/Requirement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
